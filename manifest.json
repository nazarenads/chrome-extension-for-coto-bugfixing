{
    "manifest_version": 2,
   
    "name": "Fix bug when adding card in Coto Digital",
    "version": "0.0.1",
    "description": "Extension to fix bug when adding new card in Coto Digital",
    "permissions": ["activeTab", "declarativeContent"],
    "background": {
        "scripts": ["content.js"],
        "persistent": false
      },
    "page_action": {
        "default_popup": "popup.html"
      },
    "icons": {
      "128": "icon.png"
    }
  }