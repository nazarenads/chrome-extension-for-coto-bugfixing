{
    "manifest_version": 2,
   
    "name": "Fix bug when adding card",
    "version": "0.1.0",
    "description": "Extension to fix bug when adding new card in Coto digital",
    "permissions": ["activeTab", "declarativeContent"],
    "background": {
        "scripts": ["content.js"],
        "persistent": false
      },
    "page_action": {
        "default_popup": "popup.html"
      },
    "icons": {
      "128": "icon.png"
    }
  }